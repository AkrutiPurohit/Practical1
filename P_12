//Write C++ Program To Print Multiplication Table of a Number.

#include<iostream>

using namespace std;

int main()
{
    
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    int n,i;
    cout<<"Enter the number:";
    cin>>n;
    
    for(i=1;i<=10;i++)
    {
        cout<<n<<" * "<<i<<" = "<<n*i<<endl;
    }
    return 0;
}
