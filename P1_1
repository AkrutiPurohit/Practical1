//Write a C program to enter two numbers and find their sum.

#include<iostream>
using namespace std;
int main()
{
    cout<<"Purohit Akruti K."<<endl;
    cout<<"220130318033"<<endl;
    int a,b;
    cout<<"Enter two numbers:";
    cin>>a>>b;
    cout<<"The sum of given numbers is:"<<a+b<<endl;
    return 0;
}

/*

Purohit Akruti K.
